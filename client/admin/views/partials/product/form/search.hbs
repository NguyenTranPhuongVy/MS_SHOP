<el-form :label-position="labelPositionTop" :model="searchForm" :rules="searchValidate" ref="searchForm" label-width="120px" class="demo-ruleForm" size="mini">
    <div class="row">
        <div class="col-lg-6">
            <el-form-item label="Ngày Bắt Đầu">
                <el-date-picker class="w-100" v-model="searchForm.dateStart" type="datetime" placeholder="Select date and time">
                </el-date-picker>
            </el-form-item>
        </div>
        <div class="col-lg-6">
            <el-form-item label="Ngày Kết Thúc">
                <el-date-picker class="w-100" v-model="searchForm.dateEnd" type="datetime" placeholder="Select date and time">
                </el-date-picker>
            </el-form-item>
        </div>
        <div class="col-lg-6">
            <el-form-item label="Số Tiền Bắt Đầu">
                <el-input v-model="searchForm.priceStart"></el-input>
            </el-form-item>
        </div>
        <div class="col-lg-6">
            <el-form-item label="Số Tiền Kết Thúc">
                <el-input v-model="searchForm.priceEnd"></el-input>
            </el-form-item>
        </div>
    </div>
    <div class="row">
        <div class="col-lg-12">
            <el-form-item class="float-right">
                <el-button @click="searchByProduct(searchForm)" type="danger" round>Tìm Kiếm</el-button>
            </el-form-item>
        </div>
    </div>
</el-form>
